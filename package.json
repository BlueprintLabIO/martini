{
	"name": "martini-kit",
	"private": true,
	"version": "0.0.1",
	"packageManager": "pnpm@10.22.0",
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"dev": "turbo run dev --filter=@martini-kit/web",
		"dev:demos": "turbo watch dev --filter=demos",
		"build": "turbo run build",
		"test": "turbo run test",
		"clean": "turbo run clean && rm -rf node_modules",
		"upload-starter-assets": "tsx scripts/upload-starter-assets.ts"
	},
	"pnpm": {
		"overrides": {
			"@lucide/svelte": "npm:lucide-svelte@^0.553.0"
		}
	},
	"devDependencies": {
		"@supabase/supabase-js": "^2.80.0",
		"dotenv": "^17.2.3",
		"tsx": "^4.20.6",
		"turbo": "^2.0.0",
		"typescript": "^5.3.3"
	}
}
