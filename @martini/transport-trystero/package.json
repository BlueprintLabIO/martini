{
  "name": "@martini-kit/transport-trystero",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "description": "Trystero P2P transport adapter for @martini-kit/core",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@martini-kit/core": "workspace:*",
    "trystero": "^0.22.0"
  },
  "peerDependencies": {
    "@martini-kit/core": "workspace:*"
  }
}
