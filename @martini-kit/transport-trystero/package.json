{
  "name": "@martini-kit/transport-trystero",
  "version": "0.1.1",
  "type": "module",
  "description": "P2P WebRTC transport for martini-kit. Multiplayer without networking servers.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "Apache-2.0",
  "author": "Yao <yao@blueprintlab.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/BlueprintLabIO/martini.git",
    "directory": "@martini-kit/transport-trystero"
  },
  "homepage": "https://github.com/BlueprintLabIO/martini",
  "bugs": {
    "url": "https://github.com/BlueprintLabIO/martini/issues"
  },
  "keywords": [
    "multiplayer",
